<ffsFlatFileSchema>
	<ffsSettings>
		<ffsRecordPositionType>named</ffsRecordPositionType>
		<ffsRecordSeparator>chr(13)chr(10)</ffsRecordSeparator>
		<ffsFieldSeparators>
			<ffsFieldSeparator>~</ffsFieldSeparator>
		</ffsFieldSeparators>
		<ffsPadding/>
	</ffsSettings>
	<ffsRoot>
		<Batch>
			<BatchDocuments>
				<BatchDocument>
					<PurchaseOrder>
						<TradeSimpleHeader>
							<SendersCodeForRecipient RecordPos="BEG" LPos="4"/>
						</TradeSimpleHeader>
						<PurchaseOrderHeader>
							<Buyer>
								<BuyersLocationID>
									<SuppliersCode RecordPos="GS" LPos="4"/>
								</BuyersLocationID>
							</Buyer>
							<Supplier>
								<SuppliersLocationID>
									<SuppliersCode RecordPos="GS" LPos="3"/>
								</SuppliersLocationID>
							</Supplier>
							<ShipTo>
								<ShipToLocationID>
									<BuyersCode RecordPos="N1" LPos="5"/>
								</ShipToLocationID>
							</ShipTo>
							<PurchaseOrderReferences>
								<PurchaseOrderReference RecordPos="BEG" LPos="4"/>
								<PurchaseOrderDate RecordPos="GS" LPos="5"/>
							</PurchaseOrderReferences>
							<OrderedDeliveryDetails>
								<DeliveryDate RecordPos="DTM" LPos="3"/>
							</OrderedDeliveryDetails>
						</PurchaseOrderHeader>
						<PurchaseOrderDetail>
							<PurchaseOrderLine>
								<ProductID>
									<SuppliersProductCode RecordPos="PO1" LPos="8"/>
								</ProductID>
								<ProductDescription RecordPos="PID" LPos="2"/>
								<OrderedQuantity RecordPos="PO1" LPos="3">
									<ffsAttribute1 name="UnitOfMeasure" RecordPos="PO1" LPos="4"/>
								</OrderedQuantity>
							</PurchaseOrderLine>
						</PurchaseOrderDetail>
						<PurchaseOrderTrailer>
							<NumberOfLines RecordPos="CTT" LPos="2"/>
						</PurchaseOrderTrailer>
					</PurchaseOrder>
				</BatchDocument>
			</BatchDocuments>
		</Batch>
	</ffsRoot>
</ffsFlatFileSchema>
