<ffsFlatFileSchema>
	<ffsSettings>
		<ffsRecordPositionType>named</ffsRecordPositionType>
		<ffsRecordSeparator>chr(13)chr(10)</ffsRecordSeparator>
		<!--ffsFieldSeparators>
			<ffsFieldSeparator>,</ffsFieldSeparator>
		</ffsFieldSeparators-->
		<ffsPadding>chr(32)</ffsPadding>
		<!--ffsGroups>
			<ffsGroup-->
		<!--References below which MEAN RecordPos 'H' with LPos 1 would be CODED as 'D' and LPos 14 (13+1)-->
		<!--ffsGroupRecord RecordPos="H" FieldTotal="34"/>
				<ffsSubGroupRecord RecordPos="D" FieldTotal="13"/-->
		<!--/ffsGroup>
		</ffsGroups-->
		<ffsSplitBatchRecord RecordPos="H"/>
	</ffsSettings>
	<ffsRoot>
		<Batch>
			<BatchDocuments>
				<BatchDocument>
					<Invoice>
						<TradeSimpleHeader>
							<SendersCodeForRecipient RecordPos="H" LPos="12" UPos="19"/>
						</TradeSimpleHeader>
						<InvoiceHeader>
							<Buyer>
								<!--BuyersLocationID>
									<SuppliersCode RecordPos="H" LPos="12" UPos="8"/>
								</BuyersLocationID>
								<BuyersName RecordPos="H" LPos="" UPos=""/>
								<BuyersAddress>
									<AddressLine1 RecordPos="H" LPos="" UPos=""/>
									<AddressLine2 RecordPos="H" LPos="" UPos=""/>
									<AddressLine3 RecordPos="H" LPos="" UPos=""/>
									<AddressLine4 RecordPos="H" LPos="" UPos=""/>
									<PostCode RecordPos="H" LPos="" UPos=""/>
								</BuyersAddress-->
							</Buyer>
							<Supplier>
								<!--<SuppliersLocationID>
									<SuppliersCode RecordPos="" LPos=""/>
								</SuppliersLocationID>>
								<SuppliersName RecordPos="H" LPos="" UPos=""/>
								<SuppliersAddress>
									<AddressLine1 RecordPos="H" LPos="" UPos=""/>
									<AddressLine2 RecordPos="H" LPos="" UPos=""/>
									<AddressLine3 RecordPos="H" LPos="" UPos=""/>
									<AddressLine4 RecordPos="H" LPos="" UPos=""/>
									<PostCode RecordPos="H" LPos="" UPos=""/>
								</SuppliersAddress-->
							</Supplier>
							<ShipTo>
								<ShipToLocationID>
									<SuppliersCode RecordPos="H" LPos="12" UPos="19"/>

								</ShipToLocationID>>
								<!--ShipToName RecordPos="H" LPos="12" UPos="8"/>
								<ShipToAddress>
									<AddressLine1 RecordPos="H" LPos="" UPos=""/>
									<AddressLine2 RecordPos="H" LPos="" UPos=""/>
									<AddressLine3 RecordPos="H" LPos="" UPos=""/>
									<AddressLine4 RecordPos="H" LPos="" UPos=""/>
									<PostCode RecordPos="H" LPos="" UPos=""/>
								</ShipToAddress-->
							</ShipTo>
							<InvoiceReferences>
								<InvoiceReference RecordPos="H" LPos="20" UPos="29"/>
								<InvoiceDate RecordPos="H" LPos="30" UPos="39"/>
								<TaxPointDate RecordPos="H" LPos="40" UPos="49"/>
								<!--VATRegNo RecordPos="H" LPos="" UPos=""/-->
							</InvoiceReferences>
							<!--Currency RecordPos="H" LPos="" UPos=""/-->
						</InvoiceHeader>
						<InvoiceDetail>
							<InvoiceLine>
								<!--<LineNumber RecordPos="" LPos=""/>-->
								<PurchaseOrderReferences>
									<PurchaseOrderReference RecordPos="H" LPos="50" UPos="64"/>
									<PurchaseOrderDate RecordPos="H" LPos="65" UPos="74"/>
								</PurchaseOrderReferences>
								<!--DeliveryNoteReferences>
									<DeliveryNoteReference RecordPos="D" LPos="" UPos=""/>
									<DeliveryNoteDate RecordPos="D" LPos="" UPos=""/>
									<DespatchDate RecordPos="" LPos=""/>
								</DeliveryNoteReferences-->
								<ProductID>
									<SuppliersProductCode RecordPos="D" LPos="12" UPos="21"/>
								</ProductID>
								<!--ProductDescription RecordPos="D" LPos="" UPos=""/-->
								<InvoicedQuantity RecordPos="D" LPos="22" UPos="31"/>
								<!--PackSize RecordPos="D" LPos="" UPos=""/-->
								<UnitValueExclVAT RecordPos="D" LPos="32" UPos="41"/>
								<LineValueExclVAT RecordPos="D" LPos="42" UPos="51"/>
								<VATCode RecordPos="D" LPos="52" UPos="57"/>
								<!--VATRate RecordPos="D" LPos="" UPos=""/-->
							</InvoiceLine>
						</InvoiceDetail>
						<InvoiceTrailer>
							<VATSubTotals>
								<VATSubTotal>
									<!-- These fields are just used to carry data to the translator, true semantics described below  -->
									<!-- VAT AMOUNT Z -->
									<NumberOfLinesAtRate RecordPos="H" LPos="75" UPos="84"/>
									<!-- GOODS AMOUNT Z -->
									<NumberOfItemsAtRate RecordPos="H" LPos="85" UPos="94"/>
									<!-- VAT AMOUNT S -->
									<DocumentTotalExclVATAtRate RecordPos="H" LPos="95" UPos="104"/>
									<!-- GOODS AMOUNT S -->
									<SettlementDiscountAtRate RecordPos="H" LPos="105" UPos="114"/>
								</VATSubTotal>
							</VATSubTotals>
						</InvoiceTrailer>
					</Invoice>
				</BatchDocument>
			</BatchDocuments>
		</Batch>
	</ffsRoot>
</ffsFlatFileSchema>
