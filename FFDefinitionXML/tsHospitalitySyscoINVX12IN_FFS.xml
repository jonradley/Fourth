<ffsFlatFileSchema>
	<ffsSettings>
		<ffsRecordPositionType>named</ffsRecordPositionType>
		<ffsRecordSeparator>~</ffsRecordSeparator>
		<ffsEscapeChars>
			<ffsEscapeChar>
				<ffsNative>chr(13)</ffsNative>
				<ffsTradesimple/>
				<ffsLiteral/>
			</ffsEscapeChar>
			<ffsEscapeChar>
				<ffsNative>chr(10)</ffsNative>
				<ffsTradesimple/>
				<ffsLiteral/>
			</ffsEscapeChar>
		</ffsEscapeChars>
		<ffsFieldSeparators>
			<ffsFieldSeparator>*</ffsFieldSeparator>
		</ffsFieldSeparators>
		<ffsSplitBatchRecord RecordPos="ST*810"/>
		<ffsPadding/>
	</ffsSettings>
	<ffsRoot>
		<Batch>
			<TradeSimpleHeader>
				<SendersCodeForRecipient RecordPos="N1*ST" LPos="5"/>
				<SendersBranchReference RecordPos="ISA" LPos="7"/>
			</TradeSimpleHeader>
			<BatchDocuments>
				<BatchDocument>
					<Invoice>
						<TradeSimpleHeader>
							<SendersCodeForRecipient RecordPos="N1*ST" LPos="5"/>
							<SendersBranchReference RecordPos="ISA" LPos="7"/>
						</TradeSimpleHeader>
						<InvoiceHeader>
							<!--Dummy value <DocumentStatus> to be changed in stylesheet.-->
							<DocumentStatus RecordPos="GS" LPos="3"/>
							<Buyer>
								<BuyersLocationID>
									<SuppliersCode RecordPos="GS" LPos="4"/>
								</BuyersLocationID>
							</Buyer>
							<Supplier>
								<SuppliersLocationID>
									<SuppliersCode RecordPos="GS" LPos="3"/>
								</SuppliersLocationID>
							</Supplier>
							<ShipTo>
								<ShipToLocationID>
									<SuppliersCode RecordPos="N1*ST" LPos="5"/>
								</ShipToLocationID>
							</ShipTo>
							<InvoiceReferences>
								<InvoiceReference RecordPos="BIG" LPos="3"/>
								<InvoiceDate RecordPos="BIG" LPos="2"/>
							</InvoiceReferences>
						</InvoiceHeader>
						<InvoiceDetail>
							<InvoiceLine>
								<PurchaseOrderReferences>
									<PurchaseOrderReference RecordPos="BIG" LPos="4"/>
								</PurchaseOrderReferences>
								<ProductID>
									<SuppliersProductCode RecordPos="IT1" LPos="8"/>
									<BuyersProductCode RecordPos="IT1" LPos="14"/>
								</ProductID>
								<ProductDescription RecordPos="PID" LPos="6"/>
								<InvoicedQuantity RecordPos="IT1" LPos="3">
									<ffsAttribute1 name="UnitOfMeasure" RecordPos="IT1" LPos="4"/>
								</InvoicedQuantity>
								<UnitValueExclVAT RecordPos="IT1" LPos="5"/>
								<!--LineValueExclVAT RecordPos="" LPos=""/-->
							</InvoiceLine>
						</InvoiceDetail>
					</Invoice>
				</BatchDocument>
			</BatchDocuments>
		</Batch>
	</ffsRoot>
</ffsFlatFileSchema>
