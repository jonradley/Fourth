<ffsFlatFileSchema>

	<ffsSettings>
		<ffsRecordPositionType>named</ffsRecordPositionType>
		<ffsRecordSeparator FixedSize="false">chr(13)chr(10)</ffsRecordSeparator>
		<ffsFieldSeparators>
			<ffsFieldSeparator>,</ffsFieldSeparator>
		</ffsFieldSeparators>
		<ffsSplitBatchRecord RecordPos="H"/>
	</ffsSettings>
	
	<ffsRoot>
	
		<Batch>
		
			<BatchHeader>
			
				<OrganisationCode RecordPos="E" LPos="2"/>
				<SourceSystemExportID RecordPos="E" LPos="3"/>
				<SourceSystemOrgID RecordPos="E" LPos="5"/>
				<FormatCode RecordPos="E" LPos="4"/>
				<OrganisationName RecordPos="E" LPos="6"/>
				<ExportRunDate RecordPos="E" LPos="7"/>
				<ExportRunTime RecordPos="E" LPos="8"/>
				
			</BatchHeader>
			
			
			<BatchDocuments>
			
				<BatchDocument>
				
					<Receipt>
					
						<ReceiptHeader>

							<!-- Transaction Type -->
							<DocumentType RecordPos="H" LPos="2"/>
						
							<!--  Unit Code  -->
							<BuyersUnitCode RecordPos="H" LPos="3"/>
						
							<!--  Site Code  -->
							<BuyersSiteCode RecordPos="H" LPos="4"/>
						
							<!--  Site Name  -->
							<BuyersSiteName RecordPos="H" LPos="5"/>
						
							<!--  Site Nominal Code  -->	
							<SiteNominalCode RecordPos="H" LPos="6"/>
						
							<!--  Document Reference  -->
							<DeliveryNoteReference RecordPos="H" LPos="7"/>
						
							<!--  Document Date  -->
							<DeliveryNoteDate RecordPos="H" LPos="8"/>
						
							<!--  Call Reference  -->
							<CallReference RecordPos="H" LPos="9"/>
						
							<!--  Transaction Description  -->
							<TransactionDescription RecordPos="H" LPos="10"/>
						
							<!--  Supplier Code  -->
							<BuyersCodeForSupplier RecordPos="H" LPos="11"/>
						
							<!--  Supplier Name  -->
							<SuppliersName RecordPos="H" LPos="12"/>
						
							<!--  Supplier Nominal Code  -->
							<SupplierNominalCode RecordPos="H" LPos="13"/>
						
							<!--  Order Reference  -->
							<PurchaseOrderReference RecordPos="H" LPos="14"/>
						
							<!--  Order Date  -->
							<PurchaseOrderDate RecordPos="H" LPos="15"/>
						
							<!--  Total Ex VAT  -->
							<TotalExclVAT RecordPos="H" LPos="16"/>
						
							<!--  Total VAT  -->
							<VATAmount RecordPos="H" LPos="17"/>
						
							<!--  Total Inc VAT  -->
							<TotalInclVAT RecordPos="H" LPos="18"/>
						
							<!--  Number of Lines  -->
							<NumberOfLines RecordPos="H" LPos="19"/>
						
							<!--  Comments  -->
							<Narrative RecordPos="H" LPos="20"/>
						
							<!--  Voucher Number  -->
							<VoucherNumber RecordPos="H" LPos="21"/>
						
							<!--  Approved Username  -->
							<ApprovedBy RecordPos="H" LPos="22"/>
						
							<!--  Approved Date  -->
							<ApprovedDate RecordPos="H" LPos="23"/>
						
							<!--  Transaction currency  -->
							<CurrencyCode RecordPos="H" LPos="24"/>
						
							<!--  Stock Period Year  -->
							<StockFinancialYear RecordPos="H" LPos="25"/>
						
							<!--  Stock Period  -->
							<StockFinancialPeriod RecordPos="H" LPos="26"/>
						
							<!--  Company Code  -->
							<CompanyCode RecordPos="H" LPos="27"/>
						
							<!--  DocumentID  -->
							<DocumentID RecordPos="H" LPos="28"/>
						
						</ReceiptHeader>
						
						
						<ReceiptDetail>
						
							<ReceiptLine>
							
								<!-- LineNumber -->
								<LineNumber RecordPos="D" LPos="2"/>
								
								<!-- SupplierProductCode -->
								<SuppliersProductCode RecordPos="D" LPos="3"/>
								
								<!-- Description -->
								<ProductDescription RecordPos="D" LPos="4"/>
								
								<!-- PackSize -->
								<PackSize RecordPos="D" LPos="5"/>
								
								<!-- OrderedQuantity -->
								<OrderedQuantity RecordPos="D" LPos="6"/>
								
								<!-- AcceptedQuantity -->
								<AcceptedQuantity RecordPos="D" LPos="7"/>
								
								<!-- UnitCostPrice -->
								<UnitValueExclVAT RecordPos="D" LPos="8"/>
								
								<!-- VATCode -->
								<CustomerVATCode RecordPos="D" LPos="9"/>
								
								<!-- VATRate -->
								<VATRate RecordPos="D" LPos="10"/>
								
								<!-- LineValueExVAT -->
								<LineValueExclVAT RecordPos="D" LPos="11"/>
								
								<!-- LineVAT -->
								<LineVAT RecordPos="D" LPos="12"/>
								
								<!-- LineValueIncVAT -->
								<LineValueIncVAT RecordPos="D" LPos="13"/>
								
								<!-- CategoryNominal -->
								<CategoryNominal RecordPos="D" LPos="14"/>
								
								<!-- CategoryName -->
								<CategoryName RecordPos="D" LPos="15"/>
								
								<!-- Comments -->
								<Narrative RecordPos="D" LPos="16"/>
								
								<!-- Reason -->
								<Reason RecordPos="D" LPos="17"/>							
							
							</ReceiptLine>		
											
											
						</ReceiptDetail>
					
					
					</Receipt>
					
				</BatchDocument>
				
			</BatchDocuments>
			
		</Batch>
		
	</ffsRoot>
	
</ffsFlatFileSchema>
