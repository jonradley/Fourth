<?xml version="1.0" encoding="UTF-8"?>
<ffsFlatFileSchema>
	<ffsSettings>
		<ffsRecordPositionType>named</ffsRecordPositionType>
		<ffsRecordSeparator>chr(13)chr(10)</ffsRecordSeparator>
		<!--ffsEscapeChars>
			<ffsEscapeChar>
				<ffsNative>chr(13)</ffsNative>
				<ffsTradesimple/>
				<ffsLiteral/>
			</ffsEscapeChar>
			<ffsEscapeChar>
				<ffsNative>""</ffsNative>
				<ffsTradesimple>Â¬Q</ffsTradesimple>
				<ffsLiteral>"</ffsLiteral>
			</ffsEscapeChar>
		</ffsEscapeChars-->
		<ffsFieldSeparators>
			<ffsFieldSeparator>,</ffsFieldSeparator>
		</ffsFieldSeparators>
		<!--ffsPadding/-->
		<ffsBatchHeader>
			<!--ist of records that make up the header, this will be added to each document in the batch-->
			<ffsHeaderRecord RecordPos="SYKCO"/>
		</ffsBatchHeader>
		<!--ffsGroups>
			<ffsGroup>
				<ffsGroupRecord RecordPos="SYKCO" FieldTotal="34"/>
				<ffsSubGroupRecord RecordPos="SZKCO" FieldTotal="2"/>
			</ffsGroup>
		</ffsGroups-->
		<ffsSplitBatchRecord RecordPos="SYKCO"/>
	</ffsSettings>
	<ffsRoot>
		<Batch>
			<BatchDocuments>
				<BatchDocument>
					<Invoice>
						<TradeSimpleHeader>
							<SendersCodeForRecipient RecordPos="SYKCO" LPos="10"/>
						</TradeSimpleHeader>
						<InvoiceHeader>
							<Buyer>
								<BuyersLocationID>
									<SuppliersCode RecordPos="SYKCO" LPos="2"/>
								</BuyersLocationID>
							</Buyer>
							<ShipTo>
								<ShipToLocationID>
									<SuppliersCode RecordPos="SYKCO" LPos="8"/>
								</ShipToLocationID>
							</ShipTo>
							<InvoiceReferences>
								<InvoiceReference RecordPos="SYKCO" LPos="4"/>
								<InvoiceDate RecordPos="SYKCO" LPos="8"/>
								<TaxPointDate RecordPos="SYKCO" LPos="10"/>
							</InvoiceReferences>
							<!--PurchaseOrderConfirmationReferences>
								<PurchaseOrderConfirmationReference RecordPos="SYKCO" LPos="2"/>
								<PurchaseOrderConfirmationDate RecordPos="SYKCO" LPos="10"/>
							</PurchaseOrderConfirmationReferences>
							<ConfirmedDeliveryDetails>
								<DeliveryDate RecordPos="SYKCO" LPos="10"/>
							</ConfirmedDeliveryDetails-->
						</InvoiceHeader>
						<InvoiceDetail>
							<InvoiceLine>
								<LineNumber RecordPos="SZKCO" LPos="8"/>
								<!--PurchaseOrderReferences>
									<PurchaseOrderReference RecordPos="SZKCO" LPos="20	"/>
									<PurchaseOrderDate RecordPos="SZKCO" LPos="51"/>
								</PurchaseOrderReferences>
								<DeliveryNoteReferences>
									<DeliveryNoteReference RecordPos="SZKCO" LPos="50"/>
									<DeliveryNoteDate RecordPos="SZKCO" LPos="47"/>
								</DeliveryNoteReferences-->
								<ProductID>
									<SuppliersProductCode RecordPos="SZKCO" LPos="22"/>
								</ProductID>
								<ProductDescription RecordPos="SZKCO" LPos="24"/>
								<InvoicedQuantity name="UnitOfMeasure" RecordPos="SZKCO" LPos="28"/>
								<PackSize RecordPos="SZKCO" LPos="26"/>
								<UnitValueExclVAT RecordPos="SZKCO" LPos="32"/>
								<LineValueExclVAT RecordPos="SZKCO" LPos="34"/>
								<VATCode RecordPos="SZKCO" LPos="36"/>
								<VATRate RecordPos="SZKCO" LPos="38"/>
							</InvoiceLine>
						</InvoiceDetail>
						<InvoiceTrailer>
							<!--NumberOfLines RecordPos="SZKCO" LPos="16"/-->
						</InvoiceTrailer>
					</Invoice>
				</BatchDocument>
			</BatchDocuments>
		</Batch>
	</ffsRoot>
</ffsFlatFileSchema>
