<ffsFlatFileSchema>
	<ffsSettings>
		<ffsRecordPositionType>named</ffsRecordPositionType>
		<ffsRecordSeparator>chr(10)</ffsRecordSeparator>
		<ffsEscapeChars>
			<ffsEscapeChar>
				<ffsNative>chr(13)</ffsNative>
				<ffsTradesimple/>
				<ffsLiteral/>
			</ffsEscapeChar>
		</ffsEscapeChars>
		<ffsFieldSeparators>
			<ffsFieldSeparator>,</ffsFieldSeparator>
		</ffsFieldSeparators>
		<ffsPadding/>
		<ffsBatchHeader>
			<!--list of records that make up the header, this will be added to each document in the batch -->
			<ffsHeaderRecord RecordPos="HEAD"/>
		</ffsBatchHeader>
		<ffsGroups>
			<ffsGroup>
				<!--References below which MEAN RecordPos 'HEAD' -->
				<ffsGroupRecord RecordPos="HEAD" FieldTotal="27"/>
				<ffsSubGroupRecord RecordPos="LINE" FieldTotal="30"/>
			</ffsGroup>
		</ffsGroups>
		<ffsSplitBatchRecord RecordPos="HEAD"/>
	</ffsSettings>
	<ffsRoot>
		<Batch>
			<BatchDocuments>
				<BatchDocument>
					<Invoice>
						<TradeSimpleHeader>
							<SendersCodeForRecipient RecordPos="HEAD" LPos="4"/>
							<SendersBranchReference RecordPos="HEAD" LPos="3"/>
						</TradeSimpleHeader>
						<InvoiceHeader>
							<BatchInformation>
								<FileGenerationNo RecordPos="HEAD" LPos="25"/>
								<FileVersionNo RecordPos="HEAD" LPos="26"/>
								<FileCreationDate RecordPos="HEAD" LPos="27"/>
							</BatchInformation>
							<Buyer>
								<BuyersLocationID>
									<SuppliersCode RecordPos="HEAD" LPos="4"/>
