										<ContractReference RecordPos="HEAD" LPos="40"/>
									</TradeAgreement>
								</PurchaseOrderReferences>
								<PurchaseOrderConfirmationReferences>
									<PurchaseOrderConfirmationReference RecordPos="HEAD" LPos="7"/>
								</PurchaseOrderConfirmationReferences>
								<DeliveryNoteReferences>
									<DeliveryNoteReference RecordPos="HEAD" LPos="11"/>
									<DeliveryNoteDate RecordPos="HEAD" LPos="12"/>
								</DeliveryNoteReferences>
								<ProductID>
									<GTIN RecordPos="LINE" LPos="18"/>
									<SuppliersProductCode RecordPos="LINE" LPos="2"/>
								</ProductID>
								<ProductDescription RecordPos="LINE" LPos="26"/>
								<InvoicedQuantity RecordPos="LINE" LPos="7">
									<ffsAttribute1 name="UnitOfMeasure" RecordPos="LINE" LPos="9"/>
								</InvoicedQuantity>
								<PackSize RecordPos="LINE" LPos="4"/>
								<UnitValueExclVAT RecordPos="LINE" LPos="10"/>
