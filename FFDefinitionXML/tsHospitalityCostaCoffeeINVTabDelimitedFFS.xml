<ffsFlatFileSchema>
	<ffsSettings>
		<ffsRecordPositionType>named</ffsRecordPositionType>
		<ffsRecordSeparator>chr(13)chr(10)</ffsRecordSeparator>
		<ffsFieldSeparators>
			<ffsFieldSeparator>chr(09)</ffsFieldSeparator>
		</ffsFieldSeparators>
		<ffsPadding/>
		<ffsGroups>
			<ffsGroup>
				<!--References below which mean RecordPos ODD= with LPos 1 would be coded as ILD= and LPos 26 (25+1)-->
				<ffsGroupRecord RecordPos="HEAD" FieldTotal="44"/>
				<ffsSubGroupRecord RecordPos="LINE" FieldTotal="44"/>
			</ffsGroup>
		</ffsGroups>
	</ffsSettings>
	<ffsRoot>
		<Batch>
			<BatchDocuments>
				<BatchDocument>
					<Invoice>
						<TradeSimpleHeader>
							<SendersCodeForRecipient RecordPos="HEAD" LPos="11"/>
							<!--SendersBranchReference RecordPos="" LPos=""/-->
						</TradeSimpleHeader>
						<InvoiceHeader>
							<Buyer>
								<BuyersLocationID>
									<GLN RecordPos="HEAD" LPos="4"/>
									<SuppliersCode RecordPos="HEAD" LPos="3"/>
								</BuyersLocationID>
							</Buyer>
							<Supplier>
								<SuppliersLocationID>
									<GLN RecordPos="TAB_FILE" LPos="11"/>
									<SuppliersCode RecordPos="TAB_FILE" LPos="3"/>
								</SuppliersLocationID>
							</Supplier>
							<ShipTo>
								<ShipToLocationID>
									<GLN RecordPos="HEAD" LPos="12"/>
									<SuppliersCode RecordPos="HEAD" LPos="11"/>
								</ShipToLocationID>
							</ShipTo>
							<InvoiceReferences>
								<InvoiceReference RecordPos="HEAD" LPos="22"/>
								<InvoiceDate RecordPos="HEAD" LPos="23"/>
								<TaxPointDate RecordPos="HEAD" LPos="24"/>
							</InvoiceReferences>
						</InvoiceHeader>
						<InvoiceDetail>
							<InvoiceLine>
								<PurchaseOrderReferences>
									<PurchaseOrderReference RecordPos="LINE" LPos="74"/>
									<PurchaseOrderDate RecordPos="LINE" LPos="75"/>
								</PurchaseOrderReferences>
								<ProductID>
									<SuppliersProductCode RecordPos="LINE" LPos="2"/>
								</ProductID>
								<InvoicedQuantity RecordPos="LINE" LPos="8">
									<ffsattribute1 name="UnitOfMeasure" RecordPos="LINE" LPos="7"/>
								</InvoicedQuantity>
								<UnitValueExclVAT RecordPos="LINE" LPos="13"/>
								<LineValueExclVAT RecordPos="LINE" LPos="14"/>
								<VATCode RecordPos="LINE" LPos="18"/>
								<VATRate RecordPos="LINE" LPos="19"/>
							</InvoiceLine>
						</InvoiceDetail>
						<InvoiceTrailer>
							<VATSubTotals>
								<VATSubTotal>
									<ffsAttribute1 name="VATCode" RecordPos="VAT" LPos="2"/>
									<ffsAttribute1 name="VATRate" RecordPos="VAT" LPos="3"/>
									<NumberOfLinesAtRate RecordPos="VAT" LPos="4"/>
									<DocumentTotalExclVATAtRate RecordPos="VAT" LPos="5"/>
									<SettlementTotalExclVATAtRate RecordPos="VAT" LPos="5"/>
									<VATAmountAtRate RecordPos="VAT" LPos="13"/>
									<DocumentTotalInclVATAtRate RecordPos="" LPos=""/>
									<SettlementTotalInclVATAtRate RecordPos="" LPos=""/>
								</VATSubTotal>
							</VATSubTotals>
							<DocumentTotalExclVAT RecordPos="TRAILER" LPos="4"/>
							<SettlementTotalExclVAT RecordPos="TRAILER" LPos="4"/>
							<VATAmount RecordPos="TRAILER" LPos="10"/>
							<DocumentTotalInclVAT RecordPos="TRAILER" LPos="11"/>
							<SettlementTotalInclVAT RecordPos="TRAILER" LPos="11"/>
						</InvoiceTrailer>
					</Invoice>
				</BatchDocument>
			</BatchDocuments>
		</Batch>
	</ffsRoot>
</ffsFlatFileSchema>
