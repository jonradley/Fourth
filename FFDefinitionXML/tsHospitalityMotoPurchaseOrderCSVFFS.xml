<?xml version="1.0" encoding="UTF-8"?>
<!--******************************************************************
CSV Purchase Order mapperfor Moto
**********************************************************************
Name         	| Date       		| Change
*********************************************************************
A Sheppard	| 16/07/2007 	| Created
*******************************************************************-->
<ffsFlatFileSchema>
	<ffsSettings>
		<ffsRecordPositionType>named</ffsRecordPositionType>
		<ffsRecordSeparator>chr(13)chr(10)</ffsRecordSeparator>
		<ffsFieldSeparators>
			<ffsFieldSeparator>,</ffsFieldSeparator>
		</ffsFieldSeparators>
	</ffsSettings>
	<ffsRoot>
		<PurchaseOrder>
			<TradeSimpleHeader>
				<SendersCodeForRecipient RecordPos="&quot;H&quot;" LPos="2"/>
				<SendersBranchReference RecordPos="&quot;H&quot;" LPos="8"/>
			</TradeSimpleHeader>
			<PurchaseOrderHeader>
				<ShipTo>
					<ContactName RecordPos="&quot;H&quot;" LPos="7"/>
				</ShipTo>
				<PurchaseOrderReferences>
					<PurchaseOrderReference RecordPos="&quot;H&quot;" LPos="6"/>
					<PurchaseOrderDate RecordPos="&quot;H&quot;" LPos="4"/>
				</PurchaseOrderReferences>
				<OrderedDeliveryDetails>
					<DeliveryDate RecordPos="&quot;H&quot;" LPos="5"/>
				</OrderedDeliveryDetails>
			</PurchaseOrderHeader>
			<PurchaseOrderDetail>
				<PurchaseOrderLine>
					<LineNumber RecordPos="&quot;D&quot;" LPos="5"/>
					<ProductID>
						<SuppliersProductCode RecordPos="&quot;D&quot;" LPos="3"/>
						<BuyersProductCode RecordPos="&quot;D&quot;" LPos="2"/>
					</ProductID>
					<ProductDescription RecordPos="&quot;D&quot;" LPos="4"/>
					<OrderedQuantity RecordPos="&quot;D&quot;" LPos="6"/>
					<PackSize RecordPos="&quot;D&quot;" LPos="7"/>
				</PurchaseOrderLine>
			</PurchaseOrderDetail>
		</PurchaseOrder>
	</ffsRoot>
</ffsFlatFileSchema>
