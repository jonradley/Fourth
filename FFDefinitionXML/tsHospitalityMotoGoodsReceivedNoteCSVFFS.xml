<!--******************************************************************
CSV Goods Received Note mapperfor Moto
**********************************************************************
Name         	| Date       		| Change
*********************************************************************
Rave Tech	| 30/12/2008 	| Created
*******************************************************************-->
<ffsFlatFileSchema>
	<ffsSettings>
		<ffsRecordPositionType>named</ffsRecordPositionType>
		<ffsRecordSeparator>chr(13)chr(10)</ffsRecordSeparator>
		<ffsFieldSeparators>
			<ffsFieldSeparator>,</ffsFieldSeparator>
		</ffsFieldSeparators>
		<ffsSplitBatchRecord LPos="4"/>
	</ffsSettings>
	<ffsRoot>
		<GoodsReceivedNote>
			<TradeSimpleHeader>
				<SendersCodeForRecipient RecordPos="" LPos="3"/>
				<SendersBranchReference RecordPos="" LPos="1"/>
			</TradeSimpleHeader>
			<GoodsReceivedNoteHeader>
				<DocumentStatus RecordPos="" LPos="3"/>
				<Buyer>
					<BuyersLocationID>
						<!--Set dummy value to GLN in stylesheet.-->
						<GLN RecordPos="" LPos="3"/>
					</BuyersLocationID>
				</Buyer>
				<Supplier>
					<SuppliersLocationID>
					<GLN RecordPos="" LPos="3"/>
						<BuyersCode RecordPos="" LPos="3"/>
					</SuppliersLocationID>
				</Supplier>
				<ShipTo>
					<ShipToName RecordPos="" LPos="2"/>
					<ShipToLocationID>
						<GLN RecordPos="" LPos="3"/>
						<BuyersCode RecordPos="" LPos="1"/>
					</ShipToLocationID>
					<ShipToAddress>
						<AddressLine1 RecordPos="" LPos="3"/>
					</ShipToAddress>
				</ShipTo>
				<PurchaseOrderReferences>
					<PurchaseOrderReference RecordPos="" LPos="4"/>
					<!--Set PO date same as GRN Date-->
					<PurchaseOrderDate RecordPos="" LPos="5"/>
				</PurchaseOrderReferences>
				<PurchaseOrderConfirmationReferences>
					<PurchaseOrderConfirmationReference RecordPos="" LPos="4"/>
					<!--Set PO date same as GRN Date-->
					<PurchaseOrderConfirmationDate RecordPos="" LPos="5"/>
				</PurchaseOrderConfirmationReferences>
				<DeliveryNoteReferences>
					<!--Set delivery reference same as PO reference-->
					<DeliveryNoteReference RecordPos="" LPos="4"/>
					<!--Set delivery note date same as GRN date-->
					<DeliveryNoteDate RecordPos="" LPos="5"/>
					<!--Set despatch note date same as GRN date-->
					<DespatchDate RecordPos="" LPos="5"/>
				</DeliveryNoteReferences>
				<GoodsReceivedNoteReferences>
					<GoodsReceivedNoteReference RecordPos="" LPos="5"/>
					<GoodsReceivedNoteDate RecordPos="" LPos="5"/>
				</GoodsReceivedNoteReferences>
				<DeliveredDeliveryDetails>
					<DeliveryType RecordPos="" LPos="4"/>
		 			<DeliveryDate RecordPos="" LPos="5"/>
				</DeliveredDeliveryDetails>
				<ReceivedDeliveryDetails>
					<DeliveryType RecordPos="" LPos="4"/>
					<DeliveryDate RecordPos="" LPos="5"/>
				</ReceivedDeliveryDetails>
			</GoodsReceivedNoteHeader>
			<GoodsReceivedNoteDetail>
				<GoodsReceivedNoteLine>
					<ffsAttribute1 name="LineStatus" RecordPos="" LPos="14"/>
					<LineNumber RecordPos="" LPos="1" />
					<ProductID>
						<!--Set GTIN same as supplier product code-->
						<GTIN RecordPos="" LPos="6"/>
						<SuppliersProductCode RecordPos="" LPos="6"/>
					</ProductID>
					<ProductDescription RecordPos="" LPos="7"/>
					<!--Set OrderedQuantity same as DeliveredQuantity-->
					<OrderedQuantity RecordPos="" LPos="8">
						<ffsAttribute1 name="UnitOfMeasure" RecordPos="" LPos="10"/>
					</OrderedQuantity>
					<!--Set ConfirmedQuantity same as DeliveredQuantity-->
					<ConfirmedQuantity RecordPos="" LPos="8">
						<ffsAttribute1 name="UnitOfMeasure" RecordPos="" LPos="10"/>
					</ConfirmedQuantity>
					<DeliveredQuantity RecordPos="" LPos="8">
						<ffsAttribute1 name="UnitOfMeasure" RecordPos="" LPos="10"/>
					</DeliveredQuantity>
					<AcceptedQuantity RecordPos="" LPos="9">
						<ffsAttribute1 name="UnitOfMeasure" RecordPos="" LPos="10"/>
					</AcceptedQuantity>
					<PackSize RecordPos="" LPos="11"/>
					<UnitValueExclVAT RecordPos="" LPos="12"/>
					<LineValueExclVAT RecordPos="" LPos="13"/>
					<Narrative RecordPos="" LPos="15"/>
				</GoodsReceivedNoteLine>
			</GoodsReceivedNoteDetail>
			<GoodsReceivedNoteTrailer>
				<!--Set dummy value to NumberOfLines and correct it in stylesheet.-->
				<NumberOfLines RecordPos="" LPos="1"/>
				<!--Set dummy value to DiscountedLinesTotalExclVAT and correct it in stylesheet.-->
				<DiscountedLinesTotalExclVAT RecordPos="" LPos="1"/>
				<!--Set dummy value to DocumentDiscount and correct it in stylesheet.-->
				<DocumentDiscount RecordPos="" LPos="1"/>
				<!--Set dummy value to TotalExclVAT and correct it in stylesheet.-->
				<TotalExclVAT RecordPos="" LPos="1"/>
			</GoodsReceivedNoteTrailer>
		</GoodsReceivedNote>
	</ffsRoot>
</ffsFlatFileSchema>
