<!--******************************************************************
CSV Goods Received Note mapperfor Moto
**********************************************************************
Name         	| Date       		| Change
*********************************************************************
Rave Tech	| 30/12/2008 	| Created
*******************************************************************-->
<ffsFlatFileSchema>
	<ffsSettings>
		<ffsRecordPositionType>named</ffsRecordPositionType>
		<ffsRecordSeparator>chr(13)chr(10)</ffsRecordSeparator>
		<ffsFieldSeparators>
			<ffsFieldSeparator>,</ffsFieldSeparator>
		</ffsFieldSeparators>
		<ffsSplitBatchRecord LPos="4"/>
	</ffsSettings>
	<ffsRoot>
		<GoodsReceivedNote>
			<TradeSimpleHeader>
				<SendersCodeForRecipient RecordPos="" LPos="3"/>
				<SendersBranchReference RecordPos="" LPos="1"/>
			</TradeSimpleHeader>
			<GoodsReceivedNoteHeader>
				<DocumentStatus RecordPos="" LPos="3"/>
				<Supplier>
					<SuppliersLocationID>
						<BuyersCode RecordPos="" LPos="3"/>
					</SuppliersLocationID>
				</Supplier>
				<ShipTo>
					<ShipToName RecordPos="" LPos="2"/>
					<ShipToLocationID>
						<BuyersCode RecordPos="" LPos="1"/>
					</ShipToLocationID>
				</ShipTo>
				<PurchaseOrderReferences>
					<PurchaseOrderReference RecordPos="" LPos="4"/>
					<!--Set PO date same as GRN Date-->
					<PurchaseOrderDate RecordPos="" LPos="5"/>
				</PurchaseOrderReferences>
				<DeliveryNoteReferences>
					<!--Set delivery reference same as PO reference-->
					<DeliveryNoteReference RecordPos="" LPos="4"/>
					<!--Set delivery note date same as GRN date-->
					<DeliveryNoteDate RecordPos="" LPos="5"/>
					<!--Set despatch note date same as GRN date-->
					<DespatchDate RecordPos="" LPos="5"/>
				</DeliveryNoteReferences>
				<GoodsReceivedNoteReferences>
					<GoodsReceivedNoteDate RecordPos="" LPos="5"/>
				</GoodsReceivedNoteReferences>
			</GoodsReceivedNoteHeader>
			<GoodsReceivedNoteDetail>
				<GoodsReceivedNoteLine>
					<ffsAttribute1 name="LineStatus" RecordPos="" LPos="14"/>
					<ProductID>
						<SuppliersProductCode RecordPos="" LPos="6"/>
					</ProductID>
					<ProductDescription RecordPos="" LPos="7"/>
					<DeliveredQuantity RecordPos="" LPos="8"/>
					<AcceptedQuantity RecordPos="" LPos="9">
						<ffsAttribute1 name="UnitOfMeasure" RecordPos="" LPos="10"/>
					</AcceptedQuantity>
					<PackSize RecordPos="" LPos="11"/>
					<UnitValueExclVAT RecordPos="" LPos="12"/>
					<LineValueExclVAT RecordPos="" LPos="13"/>
					<Narrative RecordPos="" LPos="15"/>
				</GoodsReceivedNoteLine>
			</GoodsReceivedNoteDetail>
		</GoodsReceivedNote>
	</ffsRoot>
</ffsFlatFileSchema>
