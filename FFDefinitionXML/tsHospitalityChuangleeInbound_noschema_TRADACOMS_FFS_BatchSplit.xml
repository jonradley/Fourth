	<ffsRoot>
		<L1>
			<Delim>chr(13)</Delim>
			<ElementName Start="1" Length="4"/>
			<L2>
				<Delim>,</Delim>
			</L2>
		</L1>
	</ffsRoot>
</ffsFlatFileSchema>
