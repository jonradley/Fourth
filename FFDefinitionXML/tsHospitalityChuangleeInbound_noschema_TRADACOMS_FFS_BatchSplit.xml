			</ffsEscapeChar>
			<ffsEscapeChar>
				<ffsNative>??</ffsNative>
				<ffsTradesimple>¬Q</ffsTradesimple>
				<ffsLiteral>?</ffsLiteral>
			</ffsEscapeChar>
			<ffsEscapeChar>
				<ffsNative>chr(13)</ffsNative>
