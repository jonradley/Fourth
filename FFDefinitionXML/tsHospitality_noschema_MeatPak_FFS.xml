<ffsFlatFileSchema>
	<ffsSettings>
		<ffsRecordPositionType>noschema</ffsRecordPositionType>
	</ffsSettings>
	<ffsRoot>
		<L1>
			<Delim>chr(13)chr(10)</Delim>
			<ElementName Start="1" Length="1"/>
			<L2>
				<Delim>|</Delim>
			</L2>
		</L1>
	</ffsRoot>
</ffsFlatFileSchema>
